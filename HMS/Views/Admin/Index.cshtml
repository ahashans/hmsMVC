@model HMS.ViewModel.AdminIndexViewModel
@{
    ViewBag.Title = "Index";
}

<h2>Admin Home</h2>
<div class="">
    <div class="jumbotron">
        <h3>All Users</h3>
        <table class="table table-bordered table-responsive table-hover table-striped">
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Role</th>
            </tr>
            @foreach (var user in Model.UserList)
            {
                <tr>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                    <td>@user.Role</td>
                </tr>
            }
        </table>
    </div>
    

</div>

